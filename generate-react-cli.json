{
  "usesTypeScript": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "components",
      "withLazy": false,
      "withStory": true,
      "withStyle": false,
      "withTest": false,
      "withMock": true,
      "customTemplates": {
        "component": "components/templates/base/TemplateName.tsx",
